{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "lint": "eslint **/*.{js,jsx} --ext .js",
    "lint-fix": "eslint **/*.{js,jsx} --ext .js --fix",
    "test": "jest"
  },
  "jest": {
    "preset": "jest-expo",
